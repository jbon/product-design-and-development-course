  keywords = {hacker,internet,logiciel libre,trajectoire,institution},
  pages = {21-42},
  file = {C:\\Users\\jb2971\\Documents\\L_Travail\\Biblio\\Zotero\\storage\\6B9DHA9Z\\depoorter - Ce que le “Libre” fait aux “libristes”.docx}
