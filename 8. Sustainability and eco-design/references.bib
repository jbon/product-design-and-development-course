  abstract = {The participation of spatially distributed individuals in the whole production cycle is feasible through the transnational possibilities of information, communication, and production technologies. To a much greater extent than ever before value creation is generated through the use of knowledge. Open Production is a concept which enables companies to apply the criterion of openness to the whole value creation process. These new patterns of value creation (bottom-up-economics) enable the realization of small firms, which combine the three production factors - labor, ground and capital - in one stakeholder. This article addresses the social aspect of sustainability and gives an overview on the chances of micro-factories to foster social sustainability in manufacturing and redirect development efforts towards a collaboration-oriented rather than a growth-oriented approach.},
  journal = {Procedia CIRP},
  author = {Basmer, S. and {Buxbaum-Conradi}, S. and Krenz, P. and Redlich, T. and Wulfsberg, J. P. and Bruhns, F. -L.},
  year = {2015},
  keywords = {Social Sustainability,Open Production,Square Foot Manufacturing,Micro-Factories,Knowledge Transfer,Value creation},
  pages = {46-51},
  file = {C:\\Users\\jb2971\\Documents\\L_Travail\\Biblio\\Zotero\\storage\\CH9QJFVB\\Basmer et al. - 2015 - Open Production Chances for Social Sustainability.pdf;C:\\Users\\jb2971\\Documents\\L_Travail\\Biblio\\Zotero\\storage\\V2ZGAJRB\\S2212827114009159.html}
