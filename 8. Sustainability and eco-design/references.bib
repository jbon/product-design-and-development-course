  pages = {604-617},
  file = {C:\\Users\\jb2971\\Documents\\L_Travail\\Biblio\\Zotero\\storage\\YRURGBSK\\Parraguez et al. - 2015 - Information Flow Through Stages of Complex Enginee.pdf;C:\\Users\\jb2971\\Documents\\L_Travail\\Biblio\\Zotero\\storage\\TXG6AFJP\\7298074.html}
