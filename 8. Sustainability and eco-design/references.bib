  year = {2006},
  keywords = {Engineering Design,Environmental management,Manufacturing; Machines; Tools,Waste Management/Waste Technology,Computer-Aided Engineering (CAD; CAE) and Design},
  pages = {245-254},
  doi = {10.1007/1-4020-4617-0_16}
}

